{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "projDafa"
		},
		"AzureDataLakeStorage1_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1_ls'"
		},
		"AzureSqlDatabase1_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1_ls'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=proj-server.database.windows.net;Initial Catalog=proj-db;User ID=dan"
		},
		"AmazonS31_ls_properties_typeProperties_accessKeyId": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "AzureKeyVault1_ls",
					"type": "LinkedServiceReference"
				},
				"secretName": "awsaccesskey"
			}
		},
		"AzureDataLakeStorage1_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://projectsa.dfs.core.windows.net/"
		},
		"AzureDatabricks1_ls_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0302-115523-p0tjdba6"
		},
		"AzureKeyVault1_ls_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://projkvnew.vault.azure.net/"
		},
		"trigger1_properties_pipeline1_parameters_filename": {
			"type": "string",
			"defaultValue": "@triggerBody().filename"
		},
		"trigger1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/f2576c3d-5fc4-4fa0-9a51-7c72d84cc4b0/resourceGroups/proj-rg/providers/Microsoft.Storage/storageAccounts/projectsa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "projDafa_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_ls_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_ls_accountKey')]"
					},
					"AzureSqlDatabase1_ls_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_ls_connectionString')]"
					},
					"AzureDataLakeStorage1_ls_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_ls_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_ls_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_ls_properties_typeProperties_baseUrl')]"
					},
					"AmazonS31_ls_properties_typeProperties_accessKeyId": {
						"value": "[parameters('AmazonS31_ls_properties_typeProperties_accessKeyId')]"
					},
					"AzureDatabricks1_ls_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_ls_properties_typeProperties_existingClusterId')]"
					},
					"trigger1_properties_pipeline1_parameters_filename": {
						"value": "[parameters('trigger1_properties_pipeline1_parameters_filename')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}